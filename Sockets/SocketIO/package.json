{
  "name": "socketio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "npm run clean && npm run create && npm run dev",
    "dev": "cd src && nodemon index.ts",
    "create": "npx tsc",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint": "eslint . --ext .ts",
    "prettier:list": "prettier --list-different \"./{test,src}/**/*.ts\" || ECHO .",
    "prettier:write": "prettier --write  \"./{test,src}/**/*.ts\"",
    "prettier": "prettier --check  \"./{test,src}/**/*.ts\"",
    "clean": "rimraf dist && rimraf coverage && rimraf %npm_package_name%.zip",
    "build": "npm run build-app && copy package.json dist && cd dist && npm run install-prod && cd ../ && npm run zip",
    "build-app": "npm run clean && tsc -p tsconfig.app.json",
    "postbuild": "move %npm_package_name%.zip dist/",
    "zip": "bestzip %npm_package_name%.zip dist/",
    "install-prod": "npm i --production",
    "pre-commit": "npm run prettier:write && npm run lint:fix"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.4",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "bestzip": "^2.2.1",
    "eslint": "^8.45.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "express": "^4.18.2",
    "install": "^0.13.0",
    "nodemon": "^3.0.1",
    "npm": "^9.8.1",
    "prettier": "^3.0.0",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "socket.io": "^4.7.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.10.0"
  }
}
